{
   "name": "@project/db",
   "version": "0.1.0",
   "private": true,
   "type": "module",
   "browser": {
      "crypto": false
   },
   "exports": {
      ".": {
         "types": "./dist/index.d.ts",
         "default": "./src/index.ts"
      },
      "./client": {
         "types": "./dist/client.d.ts",
         "default": "./src/client.ts"
      },
      "./utils": {
         "types": "./dist/utils.d.ts",
         "default": "./src/utils.ts"
      },
      "./schema/*": {
         "types": "./dist/schema/*.d.ts",
         "default": "./src/schema/*.ts"
      }
   },
   "license": "MIT",
   "scripts": {
      "build": "tsc",
      "clean": "rm -rf .turbo node_modules",
      "typecheck": "tsc --noEmit --emitDeclarationOnly false",
      "drop": "bun run --env-file ../../.env drizzle-kit drop",
      "generate": "bun run --env-file ../../.env drizzle-kit generate",
      "migrate": "bun run --env-file ../../.env drizzle-kit migrate",
      "studio": "bun run --env-file ../../.env drizzle-kit studio"
   },
   "dependencies": {
      "base-x": "^5.0.0",
      "drizzle-orm": "^0.38.3",
      "drizzle-zod": "^0.6.1",
      "postgres": "^3.4.5",
      "zod": "^3.23.8"
   },
   "devDependencies": {
      "@project/tsconfig": "workspace:*",
      "drizzle-kit": "^0.30.1"
   }
}
